{
  "name": "calculator-api",
  "version": "1.0.0",
  "description": "Basic application that performs math operation and generate random strings",
  "main": "index.js",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules --respawn src/shared/infra/http/server.ts",
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files",
    "typeorm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migrate": "npm run typeorm -- -d ./src/shared/infra/typeorm/index.ts migration:run",
    "test": "NODE_ENV=test jest --runInBand --detectOpenHandles",
    "seed:operations": "ts-node-dev -r tsconfig-paths/register  src/shared/infra/typeorm/seed/operation.ts",
    "seed:users": "ts-node-dev -r tsconfig-paths/register  src/shared/infra/typeorm/seed/users.ts"
  },
  "keywords": [],
  "author": "Deco Oliveira | deco.oliveira@gmail.com",
  "license": "ISC",
  "engines":{
    "node": "18"
  },
  "dependencies": {
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.0",
    "pg": "^8.11.0",
    "redis": "^4.6.6",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.6.3",
    "ts-node-dev": "^2.0.0",
    "tsyringe": "^4.7.0",
    "typeorm": "^0.3.16"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.5",
    "@babel/core": "^7.21.8",
    "@babel/node": "^7.20.7",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.21.0",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-typescript": "^7.21.5",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.59.6",
    "@typescript-eslint/parser": "^5.59.6",
    "babel-plugin-module-resolver": "^5.0.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "eslint": "^8.40.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  }
}
